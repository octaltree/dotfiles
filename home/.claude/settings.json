{
  "permissions": {
    "allow": [
      "List(*)",
      "Fetch(https://*)",
      "Bash(ls:*)",
      "Bash(rg:*)",
      "Bash(find:*)",
      "Bash(mv:*)",
      "Bash(gemini --prompt)",
      "Bash(git commit:*)",
      "Bash(git branch:*)",
      "Bash(git diff:*)"
    ],
    "deny": []
  },
  "includeCoAuthoredBy": false,
  "env": {},
  "mcp": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp"],
      "env": {
        "PLAYWRIGHT_HEADLESS": "true"
      }
    }
  }
}
